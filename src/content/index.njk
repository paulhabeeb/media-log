---
layout: layouts/base.njk
permalink: "/{% if pagination.pageNumber > 0 %}page/{{ pagination.pageNumber + 1 }}/{% endif %}index.html"
pagination:
    data: supabasePosts
    size: 100
    reverse: true
eleventyNavigation:
    key: Log
    order: 1
---
<div class="logHeader">
    <p class="headerDescription">{{ metadata.description }}</p>
    {% getFilters supabasePosts %}
</div>

{% getPostsByFilter pagination.items %}

{% getLogPagination pagination %}

<script>{% include "js/main.js" | assetPath %}</script>
