{
    "name": "input-log",
    "version": "1.1.7",
    "description": "A log of everything I watch and read.",
    "author": "Paul Habeeb",
    "private": true,
    "browserslist": [
        "defaults",
        "IE 10",
        "IE 11"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production npm-run-all -l clean build:*",
        "build:webpack": "webpack",
        "build:eleventy": "npx @11ty/eleventy",
        "clean": "rimraf _site src/_includes/assets",
        "dev": "cross-env NODE_ENV=development npm-run-all -l clean --parallel dev:*",
        "dev:eleventy": "npx @11ty/eleventy --serve",
        "dev:webpack": "webpack --watch",
        "lint": "npm-run-all -l lint:*",
        "lint:js": "eslint .",
        "lint:sass": "stylelint src/assets/scss/*.scss",
        "lint2:njk": "djlint src --lint --profile=nunjucks --extension njk"
    },
    "devDependencies": {
        "@11ty/eleventy": "^1.0.2",
        "@11ty/eleventy-navigation": "^0.3.4",
        "@11ty/eleventy-plugin-rss": "^1.2.0",
        "@babel/core": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "autoprefixer": "^10.4.8",
        "babel-loader": "^8.2.5",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "html-minifier": "^4.0.0",
        "luxon": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.16",
        "postcss-loader": "^7.0.1",
        "pug-plugin": "^4.1.1",
        "rimraf": "^3.0.2",
        "sass": "^1.54.4",
        "sass-loader": "^13.0.2",
        "slugify": "^1.6.5",
        "style-loader": "^3.3.1",
        "stylelint": "^14.10.0",
        "stylelint-config-prettier-scss": "0.0.1",
        "stylelint-config-standard-scss": "^5.0.0",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-manifest-plugin": "^5.0.0"
    }
}
